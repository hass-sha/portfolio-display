{
  "name": "application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "client-start": "ng serve --host 10.0.0.235 --port 4205",
    "client-start-local": "ng serve --port 4205",
    "client-build": "ng build --prod --buildOptimizer=true --progress=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server-prebuild": "tslint -c server/tslint.json -p server/tsconfig.json --fix",
    "server-build": "tsc --p server",
    "postinstall": "npm run client-build && npm run server-prebuild && npm run server-build",
    "start": "node server/.",
    "nodemon-local-init": "npm run postinstall && npm start ",
